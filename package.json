    "scripts": {
        "clean": "rimraf target",
        "sync-version": "sync-pom-version pom.xml",
        "build": "SET NODE_ENV=production && webpack --mode production",
        "start": "webpack serve --port 8081 --mode development",
        "clean-jest": "jest --clearCache",
        "test": "jest --no-cache 2>&1",
        "lint": "eslint **/*.js && tslint --project .",
        "add-locale": "lingui add-locale",
        "extract-locale": "lingui extract --clean --overwrite",
        "compile-locale": "lingui compile",
        "storybook": "start-storybook -c src/test/javascript/storybook -p 9001",
        "build-storybook": "build-storybook -c src/test/javascript/storybook -o target/webapp/storybook 2>&1"
    },
    "jest": {
        "testMatch": [
            "<rootDir>/src/test/javascript/**/*Jest.js",
            "<rootDir>/src/test/javascript/**/*Jest.tsx"
        ],
        "verbose": true,
        "transform": {
            "\\.js$": "babel-jest",
            "\\.ts$": "ts-jest",
            "\\.tsx$": "ts-jest"
        },
        "setupFilesAfterEnv": [
            "<rootDir>/src/test/javascript/config/jest-setup.js"
        ],
        "moduleNameMapper": {
            "^assets(.*)$": "<rootDir>/src/main/assets/$1",
            "^main(.*)$": "<rootDir>/src/main/javascript/$1",
            "^test(.*)$": "<rootDir>/src/test/javascript/$1",
            "\\.(png|jpg|gif|svg)$": "<rootDir>/src/test/javascript/config/fileMock.js",
            "\\.(woff|woff2|eot|ttf)$": "<rootDir>/src/test/javascript/config/fileMock.js",
            "\\.(css|scss)$": "<rootDir>/src/test/javascript/config/styleMock.js"
        },
        "moduleFileExtensions": [
            "ts",
            "js",
            "tsx",
            "json"
        ],
        "globals": {
            "ts-jest": {
                "tsConfig": {
                    "jsx": "react"
                },
                "diagnostics": {
                    "warnOnly": true
                }
            }
        }
    }
